{
  "expo": {
    "name": "client",
    "description": "Bookkeeper makes it easy to keep track of books you want to read, and store notes for ones you've read.",
    "slug": "client",
    "privacy": "public",
    "sdkVersion": "31.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images//icon.png",
    "splash": {
      "image": "./src/assets/images/bookkeeper_splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#71a7a9"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    }
  }
}